{% block html_head_container %}

{% include 'html-header.twig' %}
	{% block head %}
	{% endblock %}
	</head>
{% endblock %}

	<body id="site" class="{{ post.type }} font-default slider-content antialiased" data-template="base.twig">

    {% include 'partial/btn-book-demo.twig' %}

		<div id="site-wrapper">

      {% include 'partial/nav.twig' with
				{
					'fields': post,
				}
			%}

			<section role="main" class="content-wrapper bg-pop-white">
				<div class="wrapper relative">
					{% block content %}
						Sorry, no content
					{% endblock %}
				</div>
			</section>

      {% include "templates/contact.twig" with
        {
          'theme_text_color': 'text-pop-white',
          'theme_bg_color': 'bg-pop-black',
          'theme_border_color': 'border-pop-gray'
        }
      %}

		</div>

		{% block footer %}
			<footer id="footer">
				{% include 'footer.twig' with
					{
						'fields': content,
					}
				%}
			</footer>
			{{ function('wp_footer') }}
		{% endblock %}
    <script>
      {{ include('/frontend/dist/js/main.min.js') }}
    </script>
	</body>
</html>
